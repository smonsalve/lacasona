<%= form_for @contract, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :code, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :code, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :property_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :property_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :owner_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :owner_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :lessee_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :lessee_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :initial_date, :class => 'control-label' %>
    <div class="controls">
      <%= f.date_select :initial_date, :class => 'date_select' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :final_date, :class => 'control-label' %>
    <div class="controls">
      <%= f.date_select :final_date, :class => 'date_select' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :pay_day, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :pay_day, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :rate_commission, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :rate_commission, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :active, :class => 'control-label' %>
    <div class="controls">
      <%= f.check_box :active, :class => 'check_box' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :interest_rate, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :interest_rate, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                contracts_path, :class => 'btn' %>
  </div>
<% end %>
