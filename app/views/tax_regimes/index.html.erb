<h1><%= t ".title" %></h1>

<table>
  <tr>
    <th>Name</th>
    <th>Tax percentage</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tax_regimes.each do |tax_regime| %>
  <tr>
    <td><%= tax_regime.name %></td>
    <td><%= tax_regime.tax_percentage %></td>
    <td><%= link_to t(".show"), tax_regime %></td>
    <td><%= link_to t(".edit"), edit_tax_regime_path(tax_regime) %></td>
    <td><%= link_to t(".destroy"), tax_regime, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to t(".new"), new_tax_regime_path %>
